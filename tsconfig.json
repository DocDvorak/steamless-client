{
  "compilerOptions": {
    "baseUrl": "src",
    "outDir": "dist",
    "rootDir": "src",
    "allowSyntheticDefaultImports": true,
    "declaration": false,
    "lib": [
      "esnext",
      "dom"
    ],
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "node",
    "noImplicitOverride": true,
    "sourceMap": false,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "src"
  ]
}
